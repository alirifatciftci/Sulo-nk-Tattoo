<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - süleyman</title>

    <!-- CSS Dosyaları -->
    <link rel="stylesheet" href="~/hola-master/css/base.css" />
    <link rel="stylesheet" href="~/hola-master/css/fonts.css" />
    <link rel="stylesheet" href="~/hola-master/css/main.css" />
    <link rel="stylesheet" href="~/hola-master/css/vendor.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
</head>
<body>
  

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - Ali Rıfat ÇİFTÇİ - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <!-- JS Dosyaları -->
    <script src="~/hola-master/js/jquery-3.2.1.min.js"></script>
    <script src="~/hola-master/js/main.js"></script>
    <script src="~/hola-master/js/modernizr.js"></script>
    <script src="~/hola-master/js/pace.min.js"></script>
    <script src="~/hola-master/js/plugins.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
